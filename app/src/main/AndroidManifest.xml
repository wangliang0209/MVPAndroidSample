<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wl.demo.mvpsample">

    <application android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".user.list.ListActivity" />
        <activity android:name=".user.detail.UserDetailActivity" />
        <activity android:name=".user.login.LoginActivity" />
        <activity
            android:name=".selector.DefaultSelectorActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".upload.UploadActivity" />
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
</manifest>